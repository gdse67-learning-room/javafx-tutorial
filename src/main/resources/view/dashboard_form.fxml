<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.javafxtutorial.controller.DashboardFormController">
   <children>
      <Label layoutX="167.0" layoutY="14.0" text="Sample Dashboard" textFill="#0da14f">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Button layoutX="45.0" layoutY="69.0" mnemonicParsing="false" onAction="#btnCustomerOnAction" prefHeight="40.0" prefWidth="82.0" text="Customer">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="135.0" layoutY="69.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="82.0" text="Item">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="229.0" layoutY="69.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="82.0" text="Supplier">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TextField layoutX="348.0" layoutY="85.0" promptText="search" />
   </children>
</AnchorPane>
